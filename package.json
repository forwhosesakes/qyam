{
  "name": "qyam",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run update-sprite && remix vite:build",
    "deploy": "npm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler pages dev",
    "env": "dotenv -e .dev.vars",
    "cf-typegen": "wrangler types",
    "db:generate": "dotenv -e .dev.vars npx prisma generate ",
    "db:migrate": "dotenv -e .dev.vars -- npx prisma migrate dev",
    "update-sprite": "svg-sprite -s --symbol-dest app/assets/icons/sheet --symbol-sprite sprites.svg app/assets/icons/*.svg"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.3",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/adapter-d1": "^5.22.0",
    "@prisma/adapter-neon": "^5.22.0",
    "@prisma/client": "^5.22.0",
    "@remix-run/cloudflare": "^2.13.1",
    "@remix-run/cloudflare-pages": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "better-auth": "^0.7.3",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-flat-routes": "^0.6.5",
    "resend": "^4.0.1-alpha.0",
    "sonner": "^1.7.0",
    "tailwind-merge": "^2.5.4",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@remix-run/dev": "^2.13.1",
    "@svgr/core": "^8.1.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "prisma": "^5.22.0",
    "remix-development-tools": "^4.7.5",
    "svg-sprite": "^2.0.4",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "3.57.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
